{{#each this}}
<li>
<div class="photo-card">
  <img class="image" loading="lazy" width="100%" src="{{this.webformatURL}}" alt="" data-src={{this.largeImageURL}}"/>
  <div class="stats">
    <p class="stats-item">
      <i class="material-icons">thumb_up</i>
      {{this.likes}}
    </p>
    <p class="stats-item">
      <i class="material-icons">visibility</i>
      {{this.views}}
    </p>
    <p class="stats-item">
      <i class="material-icons">comment</i>
      {{this.comments}}
    </p>
    <p class="stats-item">
      <i class="material-icons">cloud_download</i>
      {{this.downloads}}
    </p>
  </div>
</div>
</li>{{/each}}